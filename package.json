{
  "name": "squid-tracks",
  "description": "alternative splatnet2 client with ability to upload to stat.ink",
  "productName": "SquidTracks",
  "version": "1.2.0-beta",
  "private": true,
  "author": "@Wrong_Shoe",
  "devDependencies": {
    "babel-plugin-react-intl": "^2.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "foreman": "^2.0.0",
    "react-intl-translations-manager": "^5.0.3",
    "react-scripts": "^1.1.4"
  },
  "dependencies": {
    "babel-preset-electron": "^1.4.15",
    "babel-register": "^6.26.0",
    "base64url": "^2.0.0",
    "bootstrap": "3",
    "classnames": "^2.2.5",
    "concurrently": "^3.5.1",
    "electron-devtools-installer": "^2.2.3",
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.14",
    "electron-unhandled": "^1.1.0",
    "electron-updater": "^2.21.4",
    "flat": "^4.0.0",
    "history": "^4.7.2",
    "http-mitm-proxy": "^0.6.1",
    "husky": "^0.14.3",
    "immutability-helper": "^2.6.6",
    "json2csv": "^4.0.3",
    "jws": "^3.1.4",
    "lint-staged": "^7.0.4",
    "lodash": "^4.17.5",
    "moment": "^2.22.0",
    "msgpack-lite": "^0.1.26",
    "mz": "^2.7.0",
    "node-forge": "^0.7.5",
    "piping": "^1.0.0-rc.4",
    "prettier": "^1.11.1",
    "promise-memoize": "^1.2.1",
    "proptypes": "^1.1.0",
    "react": "^16.3.1",
    "react-app-rewired": "^1.5.0",
    "react-bootstrap": "^0.32.1",
    "react-broadcast": "^0.7.0",
    "react-dom": "^16.3.1",
    "react-intl": "^2.4.0",
    "react-json-tree": "^0.11.0",
    "react-polygon": "^0.1.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.2.2",
    "recharts": "^1.0.0-beta.10",
    "request-promise-native": "^1.0.4",
    "sillyname": "^0.1.0",
    "universal-analytics": "^0.4.16",
    "uuid": "^3.2.1",
    "wait-on": "^2.1.0"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "scripts": {
    "precommit": "lint-staged",
    "react-start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prepack": "yarn build",
    "pack": "build --c.extraMetadata.main=build/electron.js",
    "pack:win": "build --c.extraMetadata.main=build/electron.js --ia32 --x64",
    "release": "build -c.extraMetadata.main=build/electron.js --publish=always",
    "release:win": "build -c.extraMetadata.main=build/electron.js --ia32 --x64 --publish=always",
    "test": "react-app-rewired test --env=jsdom",
    "test:main": "jest -c=jest.main.config.json --watch",
    "eject": "react-app-rewired eject",
    "electron": "electron .",
    "electron-dev": "node public/main/dev.js .",
    "dev": "concurrently \"yarn react-start\" \"wait-on http://localhost:3000/ && yarn electron-dev\"",
    "manage:translations": "node ./translationRunner.js"
  },
  "build": {
    "appId": "com.shoesoft.splat-track",
    "directories": {
      "buildResources": "public"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ],
    "public/main/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "license": "MIT"
}
